<h1>Contacts</h1>

<section>
  <table>
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Phone</th>
        <th>Email</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @contacts.each do |current_iteration_contact| %>
        <%= erb :'contacts/_contact_row', locals: {contact: current_iteration_contact} %> <!-- locals stores `contact` for partial -->
      <% end %>
    </tbody>
  </table>
</section>
<a id="new-contact" class="btn" href="/contacts/new">New Contact</a>
